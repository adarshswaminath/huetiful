{
  "include": [
     "src/accessibility.js",
      "src/stats.js",
      "src/wrappers.js",
      "src/generators.js",
      "src/utils.js",
      "src/colors.js",
      "src/converters.js",
      "src/filterBy.js",
      "src/sortBy.js"
      
    ],"exclude":["**/*/node_modules"],
  "compilerOptions": {
    "lib":["es2023"],
    "target":"es6",
    "skipLibCheck": true,
